<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:endColor="#9AFFFFFF"
                android:startColor="#DEFFFFFF" />
            <stroke
                android:width="1dp"
                android:color="@color/textStroke" />
            <corners android:radius="14dp" />
        </shape>
    </item>

    <item
        android:bottom="16dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#00FFFFFF"
                android:startColor="#73FFFFFF" />
            <corners android:radius="13dp" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#26A9C3E8" />
            <corners android:radius="13dp" />
        </shape>
    </item>
</layer-list>
