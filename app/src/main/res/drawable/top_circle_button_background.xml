<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#CCFFFFFF"
                android:endColor="#66FFFFFF" />
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#80FFFFFF" />
        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="14dp">
        <shape android:shape="oval">
            <solid android:color="#66FFFFFF" />
        </shape>
    </item>
</layer-list>
