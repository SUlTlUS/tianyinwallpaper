<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:endColor="#73FFFFFF"
                android:startColor="#D9FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#80FFFFFF" />
            <corners android:radius="26dp" />
        </shape>
    </item>

    <item
        android:bottom="10dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#00FFFFFF"
                android:startColor="#80FFFFFF" />
            <corners android:radius="24dp" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#4DA9C3E8" />
            <corners android:radius="25dp" />
        </shape>
    </item>
</layer-list>
