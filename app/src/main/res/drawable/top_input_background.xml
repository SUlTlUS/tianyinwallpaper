<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#CFFFFFFF"
                android:endColor="#73FFFFFF" />
            <corners android:radius="28dp" />
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#80FFFFFF" />
            <corners android:radius="27dp" />
        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp">
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#80FFFFFF"
                android:endColor="#00FFFFFF" />
            <corners android:radius="26dp" />
        </shape>
    </item>
</layer-list>
